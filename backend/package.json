{
  "name": "mcp-supreme-demo-backend",
  "version": "1.0.0",
  "description": "ðŸš€ MCP Supreme Demo App - Backend with Express, MongoDB, JWT auth, and WebSocket",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'No build step required for Node.js backend'",
    "docker:build": "docker build -t mcp-supreme-demo-backend .",
    "docker:run": "docker run -p 5000:5000 mcp-supreme-demo-backend",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "mcp",
    "supreme",
    "demo",
    "express",
    "mongodb",
    "jwt",
    "websocket",
    "rest-api",
    "authentication",
    "real-time",
    "docker",
    "aws"
  ],
  "author": "MCP Supreme Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "socket.io": "^4.7.2",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.7",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
