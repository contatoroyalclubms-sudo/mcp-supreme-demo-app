  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-node": "^11.1.0",
    "@types/jest": "^29.5.3",
    "mongodb-memory-server": "^8.15.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contatoroyalclubms-sudo/mcp-supreme-demo-app.git"
  },
  "bugs": {
    "url": "https://github.com/contatoroyalclubms-sudo/mcp-supreme-demo-app/issues"
  },
  "homepage": "https://github.com/contatoroyalclubms-sudo/mcp-supreme-demo-app#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "semi": ["error", "always"]
    }
  }
}